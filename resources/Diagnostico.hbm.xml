<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.exoma.pojo.Diagnostico" table="diagnostico" catalog="public" optimistic-lock="version">
            <id name="iddia" type="integer">
            <column name="iddia" />
            <generator class="assigned" />
        </id>
        <property name="data_conclusao" type="date">
            <column name="data_conclusao" />
        </property>
        <property name="resultado" type="string">
            <column name="resultado" />
        </property>
        <property name="data_prevista" type="date">
            <column name="data_prevista" />
        </property>
        <property name="idexa" type="integer">
            <column name="idexa" />
        </property>
        <property name="idamo" type="integer">
            <column name="idamo" />
        </property>
    </class>
</hibernate-mapping>
