<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.exoma.pojo.Usuario" table="usuario" catalog="public" optimistic-lock="version">
        <id name="idusu" type="integer">
            <column name="idusu" />
            <generator class="assigned" />
        </id>
        <property name="email" type="string">
            <column name="email" />
        </property>
        <property name="senha" type="string">
            <column name="senha" />
        </property>
        <property name="nome" type="string">
            <column name="nome" />
        </property>
        <property name="ativo" type="integer">
            <column name="ativo" />
        </property>
    </class>
</hibernate-mapping>
